{
	"name": "tohezer-network",
	"version": "1.0.0",
	"description": "",
	"engines": {
		"node": ">= 9.2.0",
		"npm": "~5.5.0"
	},
	"main": "server.js",
	"scripts": {
		"test": "node node_modules/.bin/mocha --require babel-core/register --timeout 7500 server/**/*.spec.js",
		"start": "node ./server",
		"migrate:prod": "npm run db:create && npm run tables:create && npm run seed:after:create",
		"migrate:dev": "npm run db:create && npm run tables:create && npm run seed:create",
		"start:dev": "nodemon --exec ./node_modules/.bin/babel-node ./server",
		"start:prod": "./node_modules/.bin/pm2 -f start ./server.js --interpreter ./node_modules/.bin/babel-node",
		"lint:format": "eslint --fix --ext .js server client/src share server.js",
		"migration:make": "./node_modules/.bin/babel-node ./scripts/migration-make",
		"seed:make": "./node_modules/.bin/babel-node ./scripts/seed-make",
		"seed:after:create": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed --seed 20180400102909-after-create",
		"migration:run": "./node_modules/.bin/babel-node ./scripts/migration-run",
		"db:create": "./node_modules/.bin/sequelize db:create",
		"tables:create": "./node_modules/.bin/babel-node node_modules/.bin/sequelize db:migrate",
		"seed:create": "./node_modules/.bin/babel-node node_modules/.bin/sequelize db:seed:all",
		"db:drop": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:drop",
		"migrate:tables:undo": "./node_modules/.bin/babel-node node_modules/.bin/sequelize db:migrate:undo:all",
		"seed:undo:all": "./node_modules/.bin/babel-node node_modules/.bin/sequelize db:seed:undo:all"
	},
	"keywords": [
		"es6",
		"express",
		"sequelize",
		"passport authentification"
	],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "7",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-es2015-computed-properties": "^6.24.1",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"babel-watch": "^2.0.7",
		"chai": "^4.1.2",
		"eslint": "3.x",
		"mocha": "^5.0.4",
		"nodemon": "^1.12.1",
		"npm-run": "^5.0.1"
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-es2015-computed-properties": "^6.24.1",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"bcrypt": "^1.0.3",
		"body-parser": "^1.18.2",
		"bunyan": "^1.8.13",
		"compression": "^1.7.1",
		"cookie-parser": "^1.4.3",
		"express": "^4.16.0",
		"helmet": "^3.12.0",
		"jsonwebtoken": "^8.1.0",
		"lodash": "^4.17.10",
		"nodemailer": "^4.4.0",
		"npm-run": "^5.0.1",
		"passport": "^0.4.0",
		"passport-facebook": "^2.1.1",
		"passport-local": "^1.0.0",
		"pg": "6.4.1",
		"pg-hstore": "^2.3.2",
		"pm2": "^3.0.0",
		"request": "^2.85.0",
		"sequelize": "^4.22.6",
		"sequelize-cli": "^3.0.0"
	}
}
